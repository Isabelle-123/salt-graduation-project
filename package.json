{
  "name": "way-merchant-dashboard",
  "version": "1.0.0",
  "description": "A dashboard for connected merchants to get an overview of their Way checkout flow.",
  "main": "index.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "start": "npm-run-all --parallel server client",
    "test": "jest __tests__/ --maxWorkers=1",
    "watch": "jest __tests__/ --maxWorkers=1 --watch",
    "lint": "eslint .",
    "new-install": "npm i && cd client && npm i"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules/",
      "client/",
      "__tests__/testarray.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wayretail/way-merchant-dashboard.git"
  },
  "author": "Oskar Näsman, Daniel Reed, Ivar Särner, Isabelle Caspersson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wayretail/way-merchant-dashboard/issues"
  },
  "homepage": "https://github.com/wayretail/way-merchant-dashboard#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "firebase": "^7.13.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2"
  }
}
